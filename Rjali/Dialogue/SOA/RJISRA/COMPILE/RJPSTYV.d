
// Yves trades tales with Isra

EXTEND_BOTTOM RJPSTYV 0
IF ~InParty("rh#Isra2") G("RJYVIS",0)~ THEN REPLY @460 DO ~SetGlobal("RJYVIS","GLOBAL",1)~ EXTERN RH#ISRJ RJYVIS
END

CHAIN IF ~~ THEN RH#ISRJ RJYVIS
@461 = @462 = @463 = @464
== RJPSTYV @465 = @466 = @467 = @468
== RH#ISRJ @469
END
IF ~~ THEN EXTERN RJPSTYV 0

EXTEND_BOTTOM RJPSTYV 1
IF ~InParty("rh#Isra2") G("RJYVIS",0)~ THEN REPLY @460 DO ~SetGlobal("RJYVIS","GLOBAL",1)~ EXTERN RH#IS25J RJYVIST
END

CHAIN IF ~~ THEN RH#IS25J RJYVIST
@461 = @462 = @463 = @464
== RJPSTYV @465 = @466 = @467 = @468
== RH#IS25J @469
END
IF ~~ THEN EXTERN RJPSTYV 1
