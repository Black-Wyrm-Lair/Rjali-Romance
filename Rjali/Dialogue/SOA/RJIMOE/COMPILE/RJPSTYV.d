
// Yves trades tales with Imoen

EXTEND_BOTTOM RJPSTYV 0
IF ~InParty("Imoen2") G("RJYVIM",0)~ THEN REPLY @320 DO ~SetGlobal("RJYVIM","GLOBAL",1)~ EXTERN IMOEN2J RJYVIM
END

CHAIN IF ~~ THEN IMOEN2J RJYVIM
@321
== RJPSTYV @322 = @323 = @324 = @325 = @326 = @327 = @328 = @329 = @330 = @331
== RJANNAHJ IF ~InParty("RJAnnah")~ THEN @332
== RJGRACEJ IF ~InParty("RJGrace")~ THEN @333
== RJPSTYV IF ~OR(2) InParty("RJAnnah") InParty("RJGrace")~ THEN @334
END
IF ~~ THEN EXTERN RJPSTYV 0

EXTEND_BOTTOM RJPSTYV 1
IF ~InParty("Imoen2") G("RJYVIM",0)~ THEN REPLY @320 DO ~SetGlobal("RJYVIM","GLOBAL",1)~ EXTERN IMOEN25J RJYVIMT
END

CHAIN IF ~~ THEN IMOEN25J RJYVIMT
@321
== RJPSTYV @322 = @323 = @324 = @325 = @326 = @327 = @328 = @329 = @330 = @331
== RJAN25J IF ~InParty("RJAnnah")~ THEN @332
== RJGR25J IF ~InParty("RJGrace")~ THEN @333
== RJPSTYV IF ~OR(2) InParty("RJAnnah") InParty("RJGrace")~ THEN @334
END
IF ~~ THEN EXTERN RJPSTYV 1
