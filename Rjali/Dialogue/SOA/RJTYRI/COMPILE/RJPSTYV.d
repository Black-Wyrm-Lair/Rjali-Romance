
// Yves trades tales with Tyris

EXTEND_BOTTOM RJPSTYV 0
IF ~InParty("G#Tyris") G("RJYVTY",0)~ THEN REPLY @580 DO ~SetGlobal("RJYVTY","GLOBAL",1)~ EXTERN G#TYRISJ RJYVTY
END

CHAIN IF ~~ THEN G#TYRISJ RJYVTY
@581 = @582 = @583 = @584
== RJPSTYV @585 = @586 = @587 = @588 = @589
END
IF ~~ EXTERN RJPSTYV 0

EXTEND_BOTTOM RJPSTYV 1
IF ~InParty("G#Tyris") G("RJYVTY",0)~ THEN REPLY @580 DO ~SetGlobal("RJYVTY","GLOBAL",1)~ EXTERN G#TRS25J RJYVTYT
END

CHAIN IF ~~ THEN G#TRS25J RJYVTYT
@581 = @582 = @583 = @584
== RJPSTYV @585 = @586 = @587 = @588 = @589
END
IF ~~ EXTERN RJPSTYV 1
