
// Yves trades tales with Yvette

EXTEND_BOTTOM RJPSTYV 0
IF ~InParty("Yxyve") G("RJYVYV",0)~ THEN REPLY @600 DO ~SetGlobal("RJYVYV","GLOBAL",1)~ EXTERN YXYVEJ RJYVYV
END

CHAIN IF ~~ THEN YXYVEJ RJYVYV
@601
== RJPSTYV @602
== YXYVEJ @603
== RJPSTYV @604 = @605 = @606
END
IF ~~ EXTERN RJPSTYV 0

EXTEND_BOTTOM RJPSTYV 1
IF ~InParty("Yxyve") G("RJYVYV",0)~ THEN REPLY @600 DO ~SetGlobal("RJYVYV","GLOBAL",1)~ EXTERN YXYV25J RJYVYVT
END

CHAIN IF ~~ THEN YXYV25J RJYVYVT
@601
== RJPSTYV @602
== YXYV25J @603
== RJPSTYV @604 = @605 = @606
END
IF ~~ EXTERN RJPSTYV 1
