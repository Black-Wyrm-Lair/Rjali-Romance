
// Yves trades tales with Amber

EXTEND_BOTTOM RJPSTYV 0
IF ~InParty("M#AMBER") G("RJYVAM",0)~ THEN REPLY @360 DO ~SetGlobal("RJYVAM","GLOBAL",1)~ EXTERN M#AMBERJ RJYVAM
END

CHAIN IF ~~ THEN M#AMBERJ RJYVAM
@361
== RJPSTYV @362
== RJPSTYV IF ~InParty("RJAnnah")~ THEN @363
== RJPSTYV IF ~!InParty("RJAnnah")~ THEN @364 = @365 = @366
END
IF ~~ THEN EXTERN RJPSTYV 0

EXTEND_BOTTOM RJPSTYV 1
IF ~InParty("M#AMBER") G("RJYVAM",0)~ THEN REPLY @360 DO ~SetGlobal("RJYVAM","GLOBAL",1)~ EXTERN M#AMB25J RJYVAMT
END

CHAIN IF ~~ THEN M#AMB25J RJYVAMT
@361
== RJPSTYV @362
== RJPSTYV IF ~InParty("RJAnnah")~ THEN @363
== RJPSTYV IF ~!InParty("RJAnnah")~ THEN @364 = @365 = @366
END
IF ~~ THEN EXTERN RJPSTYV 1
