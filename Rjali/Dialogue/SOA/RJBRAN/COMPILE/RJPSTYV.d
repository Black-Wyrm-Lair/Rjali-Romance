
// Yves trades tales with Branwen

EXTEND_BOTTOM RJPSTYV 0
IF ~InParty("O#Bran") G("RJYVBR",0)~ THEN REPLY @380 DO ~SetGlobal("RJYVBR","GLOBAL",1)~ EXTERN O#BRANJ RJYVBR
END

CHAIN IF ~~ THEN O#BRANJ RJYVBR
@381 = @382 = @383 = @384
== RJPSTYV @385
== RJALIJ @386
== RJPSTYV @387 = @388
== O#BRANJ @389
END
IF ~~ THEN EXTERN RJPSTYV 0

EXTEND_BOTTOM RJPSTYV 1
IF ~InParty("O#Bran") G("RJYVBR",0)~ THEN REPLY @380 DO ~SetGlobal("RJYVBR","GLOBAL",1)~ EXTERN O#BRA25J RJYVBRT
END

CHAIN IF ~~ THEN O#BRA25J RJYVBRT
@381 = @382 = @383 = @384
== RJPSTYV @385
== RJALI25J @386
== RJPSTYV @387 = @388
== O#BRA25J @389
END
IF ~~ THEN EXTERN RJPSTYV 1
