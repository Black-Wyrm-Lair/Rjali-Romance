
// Yves trades tales with Saerileth

EXTEND_BOTTOM RJPSTYV 0
IF ~InParty("Saerileth") G("RJYVSA",0)~ THEN REPLY @520 DO ~SetGlobal("RJYVSA","GLOBAL",1)~ EXTERN SAERILEJ RJYVSA
END

CHAIN IF ~~ THEN SAERILEJ RJYVSA
@521 = @522 = @523
== RJPSTYV @524 = @525
== RJGRACEJ IF ~InParty("RJGrace")~ THEN @526
== RJPSTYV IF ~InParty("RJGrace")~ THEN @527
== RJPSTYV @528
END
IF ~~ THEN EXTERN RJPSTYV 0

EXTEND_BOTTOM RJPSTYV 1
IF ~InParty("Saerileth") G("RJYVSA",0)~ THEN REPLY @520 DO ~SetGlobal("RJYVSA","GLOBAL",1)~ EXTERN SAERI25J RJYVSAT
END

CHAIN IF ~~ THEN SAERI25J RJYVSAT
@521 = @522 = @523
== RJPSTYV @524 = @525
== RJGR25J IF ~InParty("RJGrace")~ THEN @526
== RJPSTYV IF ~InParty("RJGrace")~ THEN @527
== RJPSTYV @528
END
IF ~~ THEN EXTERN RJPSTYV 1
