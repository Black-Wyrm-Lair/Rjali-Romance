
// Yves trades tales with Fade

EXTEND_BOTTOM RJPSTYV 0
IF ~InParty("E3Fade") G("RJYVFA",0)~ THEN REPLY @340 DO ~SetGlobal("RJYVFA","GLOBAL",1)~ EXTERN E3FADEJ RJYVFA
END

CHAIN IF ~~ THEN E3FADEJ RJYVFA
@341
== RJPSTYV @342 = @343 = @344 = @345 = @346 = @347
== E3FADEJ @348
END
IF ~~ THEN EXTERN RJPSTYV 0

EXTEND_BOTTOM RJPSTYV 1
IF ~InParty("E3Fade") G("RJYVFA",0)~ THEN REPLY @340 DO ~SetGlobal("RJYVFA","GLOBAL",1)~ EXTERN E3FAD25J RJYVFAT
END

CHAIN IF ~~ THEN E3FAD25J RJYVFAT
@341
== RJPSTYV @342 = @343 = @344 = @345 = @346 = @347
== E3FAD25J @348
END
IF ~~ THEN EXTERN RJPSTYV 1
