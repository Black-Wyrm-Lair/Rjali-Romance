
// Yves trades tales with Ninde

EXTEND_BOTTOM RJPSTYV 0
IF ~InParty("Ninde") G("RJYVNI",0)~ THEN REPLY @480 DO ~SetGlobal("RJYVNI","GLOBAL",1)~ EXTERN LK#NINDJ RJYVNI
END

CHAIN IF ~~ THEN LK#NINDJ RJYVNI
@481
== RJPSTYV @482 = @483
== LK#NINDJ @484
END
IF ~~ THEN EXTERN RJPSTYV 0

EXTEND_BOTTOM RJPSTYV 1
IF ~InParty("Ninde") G("RJYVNI",0)~ THEN REPLY @480 DO ~SetGlobal("RJYVNI","GLOBAL",1)~ EXTERN LK#NI25J RJYVNIT
END

CHAIN IF ~~ THEN LK#NI25J RJYVNIT
@481
== RJPSTYV @482 = @483
== LK#NI25J @484
END
IF ~~ THEN EXTERN RJPSTYV 1
