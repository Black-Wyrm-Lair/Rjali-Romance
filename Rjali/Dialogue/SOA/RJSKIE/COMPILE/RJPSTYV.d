
// Yves trades tales with Skie

EXTEND_BOTTOM RJPSTYV 0
IF ~InParty("SkieDV") G("RJYVSK",0)~ THEN REPLY @540 DO ~SetGlobal("RJYVSK","GLOBAL",1)~ EXTERN 1XSKIEJ RJYVSK
END

CHAIN IF ~~ THEN 1XSKIEJ RJYVSK
@541 = @542
== RJPSTYV @543
== 1XSKIEJ @544
== RJGRACEJ IF ~InParty("RJGrace")~ THEN @545
== RJPSTYV @546
== 1XSKIEJ @547
END
IF ~~ THEN REPLY @548 EXTERN RJPSTYV 0

EXTEND_BOTTOM RJPSTYV 1
IF ~InParty("SkieDV") G("RJYVSK",0)~ THEN REPLY @540 DO ~SetGlobal("RJYVSK","GLOBAL",1)~ EXTERN 1XSKI25J RJYVSKT
END

CHAIN IF ~~ THEN 1XSKI25J RJYVSKT
@541 = @542
== RJPSTYV @543
== 1XSKI25J @544
== RJGR25J IF ~InParty("RJGrace")~ THEN @545
== RJPSTYV @546
== 1XSKI25J @547
END
IF ~~ THEN REPLY @548 EXTERN RJPSTYV 1
