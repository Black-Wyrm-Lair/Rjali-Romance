
// Yves trades tales with Petsy

EXTEND_BOTTOM RJPSTYV 0
IF ~InParty("L3Petsy") G("RJYVPE",0)~ THEN REPLY @500 DO ~SetGlobal("RJYVPE","GLOBAL",1)~ EXTERN L3PETSYJ RJYVPE
END

CHAIN IF ~~ THEN L3PETSYJ RJYVPE
@501 = @502 = @503 = @504 = @505 = @506
== RJPSTYV @507 = @508 = @509 = @510 = @511 = @512 = @513
END
IF ~~ THEN EXTERN RJPSTYV 0

EXTEND_BOTTOM RJPSTYV 1
IF ~InParty("L3Petsy") G("RJYVPE",0)~ THEN REPLY @500 DO ~SetGlobal("RJYVPE","GLOBAL",1)~ EXTERN L3PET25J RJYVPET
END

CHAIN IF ~~ THEN L3PET25J RJYVPET
@501 = @502 = @503 = @504 = @505 = @506
== RJPSTYV @507 = @508 = @509 = @510 = @511 = @512 = @513
END
IF ~~ THEN EXTERN RJPSTYV 1
